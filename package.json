{
  "name": "doombot",
  "version": "1.0.0",
  "description": "Discord bot for CS2 stats with rich embeds using Leetify API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "deploy": "npm run build && node dist/deploy-commands.js"
  },
  "keywords": [
    "discord",
    "bot",
    "cs2",
    "counter-strike",
    "leetify",
    "stats"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.1.1",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}